<%= simple_form_for @board, :html => {:multipart => true} do |f| %>
  <%= image_tag(@board.image_url, width: "300") if @board.image? %>
  <%= f.file_field :image %>
  <%= f.hidden_field :image_cache %>
  <%= f.input :description, input_html: { rows: 4, cols: 50 } %>
  <%= f.button :submit, class: "btn btn-primary"  %>
<% end %>