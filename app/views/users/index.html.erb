<h1>Users</h1>
<table class="table table-striped">
<thead>
  <tr>
    <th>#</th>
    <th>Login</th>
    <th>Email</th>
    <th>Name</th>
    <th>Birthday</th>
    <th>Country</th>
    <th>City</th>
    <th>Role</th>
  </tr>
</thead>
  <% @users.each do |user| %>
  <tr>
    <td><%= user.id %></td>
    <td><%= user.login %></td>
    <td><%= user.email %></td>
    <td><%= user.full_name %></td>
    <td><%= user.birthday %></td>
    <td><%= user.country %></td>
    <td><%= user.city %></td>
    <td><%=  %></td>
  </tr>
  <% end %>
</table>

